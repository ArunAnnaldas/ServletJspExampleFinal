



@SpringBootTest
public class LocatorPropertiesTest {

    @Autowired
    private LocatorProperties locatorProperties;

    @Test
    public void testLoadYamlProperties() {
        Map<String, String> values = locatorProperties.getValues();
        assertFalse(values.isEmpty(), "Properties should be loaded");
        System.out.println("Loaded properties: " + values);
    }
}
